<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="printers" type="TomPIT.Connected.Printing.Client.Configuration.PrintersConfigurationHandler, TomPIT.Connected.Printing.Client" />
  </configSections>  
   <appSettings>
	   <add key="cdnUrl" value="http://sys-app/core/cdn/" />
	   <add key="token" value="d69c8a22-6368-42c8-8339-3b2a7d87b366" />
    <!-- loggingLevel can be "off" < "fatal" < "error" < "warning" < "info" < "debug" < "trace" < "all" -->
    <add key="loggingLevel" value="all" />
    <!-- exceptionLoggingLevel can be "off", "errorMessage" or "stackTrace" -->
    <add key="exceptionLoggingLevel" value="stackTrace" />
	<!-- the timeout of a single print job in seconds -->
	<add key="printJobTimeout" value="30" />
  </appSettings>
  <printers>
    <printer  name="Microsoft Print to PDF"
              resourceName="PDF Printer"/>
  </printers>

</configuration>