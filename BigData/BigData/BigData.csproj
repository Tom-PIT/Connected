<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
    <AssemblyName>TomPIT.Experience.BigData</AssemblyName>
    <RootNamespace>TomPIT.BigData</RootNamespace>
    <AssemblyVersion>1.1.924.6</AssemblyVersion>
    <FileVersion>1.1.924.6</FileVersion>
    <Version>1.1.924.6</Version>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Security\**" />
    <Content Remove="Security\**" />
    <EmbeddedResource Remove="Security\**" />
